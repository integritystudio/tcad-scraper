{
	"categories": [
		{
			"title": "## ğŸš€ Features",
			"labels": ["feat", "feature", "enhancement"]
		},
		{
			"title": "## ğŸ› Bug Fixes",
			"labels": ["fix", "bugfix", "bug"]
		},
		{
			"title": "## ğŸ“š Documentation",
			"labels": ["docs", "documentation"]
		},
		{
			"title": "## ğŸ”§ Maintenance",
			"labels": ["chore", "refactor", "style"]
		},
		{
			"title": "## âš¡ Performance",
			"labels": ["perf", "performance"]
		},
		{
			"title": "## ğŸ§ª Tests",
			"labels": ["test", "tests"]
		},
		{
			"title": "## ğŸ”’ Security",
			"labels": ["security"]
		},
		{
			"title": "## ğŸ“¦ Dependencies",
			"labels": ["dependencies", "deps"]
		},
		{
			"title": "## ğŸ”„ CI/CD",
			"labels": ["ci", "build"]
		}
	],
	"ignore_labels": ["ignore", "wontfix", "duplicate", "invalid"],
	"sort": "ASC",
	"template": "${{CHANGELOG}}",
	"pr_template": "- ${{TITLE}} (#${{NUMBER}})",
	"empty_template": "- No changes",
	"label_extractor": [
		{
			"pattern": "(feat|feature):",
			"target": "$1",
			"on_property": "title"
		},
		{
			"pattern": "(fix|bugfix):",
			"target": "$1",
			"on_property": "title"
		},
		{
			"pattern": "(docs):",
			"target": "$1",
			"on_property": "title"
		}
	],
	"transformers": [
		{
			"pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)($|\\n)",
			"target": "- $4"
		}
	],
	"max_tags_to_fetch": 200,
	"max_pull_requests": 200,
	"max_back_track_time_days": 365,
	"exclude_merge_branches": ["dependabot/**"]
}
