{
  "name": "tcad-scraper-server",
  "version": "1.0.0",
  "description": "Backend API server for TCAD property scraper",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "scrape:batch": "doppler run -- tsx src/scripts/batch-scrape.ts",
    "scrape:batch:cities": "doppler run -- tsx src/scripts/batch-scrape.ts cities",
    "scrape:batch:zipcodes": "doppler run -- tsx src/scripts/batch-scrape.ts zipcodes",
    "scrape:batch:comprehensive": "doppler run -- tsx src/scripts/batch-scrape.ts comprehensive 10 5000",
    "prisma:generate": "doppler run -- prisma generate",
    "prisma:migrate": "doppler run -- prisma migrate dev",
    "prisma:studio": "doppler run -- prisma studio",
    "test": "doppler run -- jest",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@bull-board/api": "^5.10.0",
    "@bull-board/express": "^5.10.0",
    "@prisma/client": "^5.8.0",
    "@queuelabs/bullmq-utils": "^1.2.8",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "playwright": "^1.41.0",
    "redis": "^4.6.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
