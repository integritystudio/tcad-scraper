{
  "name": "tcad-scraper-server",
  "version": "1.0.0",
  "description": "Backend API server for TCAD property scraper",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "scrape:batch": "doppler run -- tsx src/scripts/batch-scrape.ts",
    "scrape:batch:cities": "doppler run -- tsx src/scripts/batch-scrape.ts cities",
    "scrape:batch:zipcodes": "doppler run -- tsx src/scripts/batch-scrape.ts zipcodes",
    "scrape:batch:comprehensive": "doppler run -- tsx src/scripts/batch-scrape.ts comprehensive 10 5000",
    "queue": "tsx src/cli/queue-manager.ts",
    "queue:status": "tsx src/cli/queue-manager.ts status",
    "queue:stop": "tsx src/cli/queue-manager.ts stop",
    "queue:cleanup": "tsx src/cli/queue-manager.ts cleanup",
    "queue:pause": "tsx src/cli/queue-manager.ts pause",
    "queue:resume": "tsx src/cli/queue-manager.ts resume",
    "analyze": "tsx src/cli/queue-analyzer.ts",
    "analyze:success": "tsx src/cli/queue-analyzer.ts success",
    "analyze:failures": "tsx src/cli/queue-analyzer.ts failures",
    "analyze:performance": "tsx src/cli/queue-analyzer.ts performance",
    "analyze:overview": "tsx src/cli/queue-analyzer.ts overview",
    "clean": "tsx src/cli/data-cleaner.ts",
    "clean:properties": "tsx src/cli/data-cleaner.ts properties-duplicates",
    "clean:queue": "tsx src/cli/data-cleaner.ts queue-duplicates",
    "clean:short": "tsx src/cli/data-cleaner.ts short-terms",
    "clean:numeric": "tsx src/cli/data-cleaner.ts numeric-terms",
    "clean:inefficient": "tsx src/cli/data-cleaner.ts inefficient-terms",
    "clean:all": "tsx src/cli/data-cleaner.ts all",
    "stats": "tsx src/cli/db-stats.ts",
    "stats:summary": "tsx src/cli/db-stats.ts summary",
    "stats:properties": "tsx src/cli/db-stats.ts properties",
    "stats:rate": "tsx src/cli/db-stats.ts rate",
    "stats:search-terms": "tsx src/cli/db-stats.ts search-terms",
    "stats:priority": "tsx src/cli/db-stats.ts priority",
    "stats:all": "tsx src/cli/db-stats.ts all",
    "prisma:generate": "doppler run -- prisma generate",
    "prisma:migrate": "doppler run -- prisma migrate dev",
    "prisma:studio": "doppler run -- prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:integration:coverage": "vitest run --coverage --config vitest.integration.config.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:all:coverage": "npm run test:unit:coverage && npm run test:integration:coverage",
    "test:security": "vitest run --config vitest.integration.config.ts src/__tests__/security.test.ts",
    "test:auth-db": "vitest run --config vitest.integration.config.ts src/__tests__/auth-database",
    "test:auth-db:connection": "vitest run --config vitest.integration.config.ts src/__tests__/auth-database.connection.test.ts",
    "test:auth-db:integration": "vitest run --config vitest.integration.config.ts src/__tests__/auth-database.integration.test.ts",
    "test:auth-db:watch": "vitest --config vitest.integration.config.ts src/__tests__/auth-database",
    "test:enqueue": "vitest run --config vitest.integration.config.ts src/__tests__/enqueue.test.ts",
    "setup:test-db": "tsx scripts/setup-test-db.ts",
    "test:token-config": "tsx src/scripts/test-api-token-config.ts",
    "test:queue-flow": "tsx src/scripts/test-queue-job-flow.ts",
    "test:token-refresh": "tsx src/scripts/test-token-refresh.ts",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@bull-board/api": "^5.10.0",
    "@bull-board/express": "^5.10.0",
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^5.8.0",
    "@queuelabs/bullmq-utils": "^1.2.8",
    "@sentry/node": "^10.23.0",
    "@sentry/profiling-node": "^10.23.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "playwright": "^1.56.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/pino": "^7.0.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/ui": "^4.0.10",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  }
}
