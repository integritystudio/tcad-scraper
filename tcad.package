{
  "name": "tcad-scraper-bullmq",
  "version": "2.0.0",
  "description": "TCAD property scraper with BullMQ job queue and monitoring",
  "main": "tcad-scraper.js",
  "scripts": {
    "start": "doppler run --command 'node tcad-scraper.js'",
