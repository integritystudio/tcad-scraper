  "keywords": [
    "tcad",
    "scraper",
    "bullmq",
    "puppeteer",
    "property-data"
  ],
  "author": "aledlie",
  "license": "MIT",
  "dependencies": {
    "bullmq": "^5.0.0",
    "puppeteer": "^21.0.0",
    "cheerio": "^1.0.0-rc.12",
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
