{
	"name": "project",
	"version": "1.0.0",
	"description": "A web scraping tool built with Puppeteer and TypeScript for extracting property tax information from the Travis Central Appraisal District (TCAD) website.",
	"main": "scraper.js",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"prebuild": "npx tsx scripts/generate-build-constants.ts",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "biome check .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"scrape": "doppler run -- tsx scraper.ts",
		"scrape:db": "doppler run -- tsx scraper-with-db.ts",
		"scrape:batch": "cd server && doppler run -- npm run scrape:batch",
		"scrape:batch:cities": "cd server && doppler run -- npm run scrape:batch:cities",
		"scrape:batch:zipcodes": "cd server && doppler run -- npm run scrape:batch:zipcodes",
		"scrape:batch:comprehensive": "cd server && doppler run -- npm run scrape:batch:comprehensive",
		"db:query": "doppler run -- tsx src/query-db.ts",
		"db:stats": "doppler run -- psql tcad_scraper -c 'SELECT COUNT(*) as total_properties, COUNT(DISTINCT city) as cities FROM properties;'",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@bull-board/api": "^6.14.0",
		"@bull-board/express": "^6.14.0",
		"@playwright/test": "^1.56.1",
		"@sentry/react": "^10.27.0",
		"@types/node": "^24.9.2",
		"@vitejs/plugin-react": "^5.0.4",
		"axios": "^1.13.2",
		"bullmq": "^5.62.0",
		"cheerio": "^1.1.2",
		"ioredis": "5.4.1",
		"mixpanel-browser": "^2.72.0",
		"pg": "^8.16.3",
		"puppeteer": "^24.27.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"vite": "^7.1.11"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.11.1",
		"@biomejs/biome": "2.3.8",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/mixpanel-browser": "^2.60.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitest/ui": "^4.0.8",
		"jsdom": "^27.1.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vitest": "^4.0.8"
	},
	"optionalDependencies": {
		"@esbuild/linux-x64": "^0.25.0",
		"@rollup/rollup-darwin-arm64": "^4.53.3",
		"@rollup/rollup-linux-x64-gnu": "^4.53.3",
		"fsevents": "^2.3.3"
	}
}
