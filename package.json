{
  "name": "project",
  "version": "1.0.0",
  "description": "A web scraping tool built with Puppeteer and TypeScript for extracting property tax information from the Travis Central Appraisal District (TCAD) website.",
  "main": "scraper.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "scrape": "tsx scraper.ts",
    "scrape:db": "tsx scraper-with-db.ts",
    "db:query": "tsx src/query-db.ts",
    "db:stats": "psql tcad_scraper -c 'SELECT COUNT(*) as total_properties, COUNT(DISTINCT city) as cities FROM properties;'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.5",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-react": "^5.0.4",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vite": "^7.1.11"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
